---
import Layout from '../../layouts/Layout.astro';
import Events from '../../components/events.astro';

const page = parseInt(Astro.url.searchParams.get('page') || '1');
const searchQuery = Astro.url.searchParams.get('search') || '';

const title = "Brisbane Events - What's On in Brisbane - Via Brisbane";
const description = "Discover upcoming events in Brisbane. Find local events, festivals, markets, and more happening in Brisbane City and surrounding areas.";
const keywords = "Brisbane events, what's on Brisbane, Brisbane festivals, Brisbane markets, Brisbane activities";
---

<Layout title={title} description={description} keywords={keywords}>
    <main>
        <Events page={page} searchQuery={searchQuery} />
    </main>
</Layout>